<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.assign.Dictionary" table="dictionary">

        <!--It's generated by Hibernate with the HiLo pattern. -->
        <id name="id" type="int" column="uid" unsaved-value="0">
				<generator class="hilo">
					<param name="table">hi_value</param>
					<param name="column">next_value</param>
				</generator>

		</id>

        <property name="word">
            <column name="words" length="25" not-null="true"/>
        </property>

		<property name="definition">
			<column name="definitions" length="200" not-null="false"/>
		</property>

    </class>

</hibernate-mapping>